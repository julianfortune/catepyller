{
	"name": "Python 3",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance",
				"ms-python.isort",
				"ms-python.mypy-type-checker",
				"matangover.mypy",
				"njpwerner.autodocstring",
				"bungcip.better-toml"
			],
			"settings": {
				"python`.defaultInterpreterPath": "/usr/local/bin/python"
			}
		}
	},
	"postCreateCommand": "poetry lock; poetry install"
}
